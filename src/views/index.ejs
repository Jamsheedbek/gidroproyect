          <div class="row">
            <% projects.forEach(project => { %>
            <div class="col-lg-4 col-md-6 wow fadeInUp" data-wow-delay="0.1s">
              <div class="service-item">
                <a class="service-img" href="/project/<%= project.id %>">
                  <img src="<%= project.imgUrl %>" alt="Image" />
                </a>
                <div class="service-text">
                  <h3><%= project.name %></h3>
                  <a
                    class="btn see-more"
                    href="<%= project.imgUrl %> "
                    data-lightbox="service"
                    ><p>&#8594;</p></a
                  >
                </div>
              </div>
            </div>
            <% }); %>
          </div>
        </div>
      </div>
